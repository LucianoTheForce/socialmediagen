# Database Configuration (Supabase PostgreSQL)
DATABASE_URL="postgresql://username:password@aws-0-region.pooler.supabase.com:5432/database-name"

# Supabase Authentication
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-supabase-anon-key"

# Redis (Upstash) - Same account as PostgreSQL
UPSTASH_REDIS_REST_URL="https://your-redis-url.upstash.io"
UPSTASH_REDIS_REST_TOKEN="your-redis-token"

# AI Services
OPENAI_API_KEY="your-openai-api-key"
RUNWARE_API_KEY="your-runware-api-key"

# Audio Services
FREESOUND_CLIENT_ID="your-freesound-client-id"
FREESOUND_API_KEY="your-freesound-api-key"

# Build Configuration (Optional)
ANALYZE="false"

# Runtime (Set automatically by Vercel)
NODE_ENV="production"
NEXT_RUNTIME="nodejs"